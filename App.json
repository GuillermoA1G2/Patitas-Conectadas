{
  "expo": {
    "name": "Patitas Conectadas",
    "slug": "patitas-conectadas",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme": "patitasconectadas",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.equipo2.patitasconectadas",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.equipo2.patitasconectadas",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VIDEO",
        "POST_NOTIFICATIONS",
        "FOREGROUND_SERVICE_MICROPHONE",
        "FOREGROUND_SERVICE_CAMERA"
      ],
      "softwareKeyboardLayoutMode": "pan",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAfgITY9iteGLbMOxZ3pkradocv8lip8GM"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permitir que Patitas Conectadas acceda a tu ubicaci√≥n para mostrar mascotas cercanas."
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "d334d412-79a8-44ec-9b30-fa4bf96b690c"
      },
      "googleMapsApiKey": "AIzaSyAfgITY9iteGLbMOxZ3pkradocv8lip8GM"
    },
    "owner": "patitas-conectadas-team"
  }
}
